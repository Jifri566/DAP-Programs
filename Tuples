print(tup2)
tup3=tup1+tup2
print(tup3)

 
tup1= ('cse','it','mech','ece','electrical')
print(tup1)
print("value at 3rd loc= ",tup1[3])
tup1[3]="hema" #not allowed ,update will not work in tuples
print("UPDATED value at 3rd loc= ",tup1[3])
 

tup1= ('cse','it','mech','ece','electrical')
print(tup1)
tup2= ('Mohiyadeen','sheikh','jifri')
print(tup2)
a=len(tup1)
b=len(tup2)
print(a)
print(b)
print(len(tup1))
print(len(tup2))
 
tup1= ('cse','it','ece','electrical','mech','xyh',)
