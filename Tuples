print(tup1)
tup2=("abc","def")
print(tup2)
tup3="hema","latha"
print(tup3)
 
tup1 = ()
print(tup1)
 
tup1 = '(24)'
print(tup1)
 

tup1 = (24)
print(tup1)
 
tup= ('cse','it','mech','ece','electrical')
